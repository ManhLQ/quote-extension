{
  "manifest_version": 2,
  "name": "Programming quotes",
  "description": "Extension for Programming quotes",
  "version": "1.0",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Programming Quotes",
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons":{
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}